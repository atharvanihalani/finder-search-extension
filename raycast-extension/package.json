{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "smart-finder-search",
  "title": "Smart Finder Search",
  "description": "Search your files with smart scoping, filtering, and ranking",
  "icon": "extension-icon.png",
  "author": "atharva",
  "categories": [
    "Productivity",
    "System"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "search-files",
      "title": "Search Files",
